{
    "name": "sh-poc-insight-frontend",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "tsc && vite build",
        "reset": "rm -rf node_modules && yarn",
        "start": "vite",
        "start-server": "tsc && vite build && node index.js",
        "start-server-no-build": "node index.js",
        "win-svc-install": "tsc && vite build && node ./service-install.js",
        "win-svc-uninstall": "node ./service-uninstall.js",
        "prepare": "husky",
        "lint": "eslint src/**/*.{ts,tsx}",
        "install-public": "yarn install --registry=https://registry.yarnpkg.com"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "dependencies": {
        "@databridge/qplus": "2.1.0-beta.263",
        "@databridge/qplus-types": "2.1.0-beta.112",
        "@emotion/react": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "@mui/icons-material": "^5.11.0",
        "@mui/lab": "^5.0.0-alpha.163",
        "@mui/material": "^5.11.8",
        "@mui/x-date-pickers": "^7.10.0",
        "@testing-library/jest-dom": "^6.4.2",
        "@vitejs/plugin-react-swc": "^3.6.0",
        "buffer": "6.0.3",
        "commitlint": "^18.6.0",
        "eslint-config-airbnb-typescript": "^17.1.0",
        "i18next": "^22.5.0",
        "js-base64": "^3.7.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-i18next": "^14.1.1",
        "react-responsive": "^10.0.0",
        "react-router-dom": "^6.23.1",
        "react-use": "^17.5.0",
        "tss-react": "^4.9.10",
        "vite": "^5.0.12",
        "vite-plugin-svgr": "^4.2.0",
        "vite-tsconfig-paths": "^4.3.1",
        "web-vitals": "3.1.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^18.6.0",
        "@commitlint/config-conventional": "^18.6.0",
        "@testing-library/react": "12.1.2",
        "@testing-library/user-event": "13.5.0",
        "@types/jest": "27.4.0",
        "@types/node": "17.0.13",
        "@types/react": "17.0.38",
        "@types/react-dom": "17.0.11",
        "@types/react-router-dom": "5.3.3",
        "@typescript-eslint/eslint-plugin": "^6.20.0",
        "@typescript-eslint/parser": "^6.20.0",
        "eslint": "^8.2.0",
        "eslint-config-airbnb": "19.0.4",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "husky": "^9.0.10",
        "lint-staged": "^15.2.1",
        "prettier": "^3.2.5",
        "react-inject-env": "^2.1.0",
        "typescript": "4.5.5",
        "vite-plugin-mkcert": "^1.17.3"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "prettier --write",
            "eslint --fix"
        ]
    },
    "resolutions": {
        "react-error-overlay ": "6.0.9",
        "@types/react": "17.0.38"
    }
}
