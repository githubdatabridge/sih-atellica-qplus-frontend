            "@libs/collaboration-providers": ["src/libs/collaboration-providers/src"],
            "@libs/collaboration-services": ["src/libs/collaboration-services/src"],
            "@libs/collaboration-ui": ["src/libs/collaboration-ui/src"],
            "@libs/common-hooks": ["src/libs/common-hooks/src"],
            "@libs/common-models": ["src/libs/common-models/src"],
            "@libs/common-providers": ["src/libs/common-providers/src"],
            "@libs/common-ui": ["src/libs/common-ui/src"],
            "@libs/common-utils": ["src/libs/common-utils/src"],
            "@libs/core-hooks": ["src/libs/core-hooks/src"],
            "@libs/core-models": ["src/libs/core-models/src"],
            "@libs/core-providers": ["src/libs/core-providers/src"],
            "@libs/core-services": ["src/libs/core-services/src"],
            "@libs/data-grid-ui": ["src/libs/data-grid-ui/src"],
            "@libs/gridwall-ui": ["src/libs/gridwall-ui/src"],
            "@libs/notification-ui": ["src/libs/notification-ui/src"],
            "@libs/qlik-base-ui": ["src/libs/qlik-base-ui/src"],
            "@libs/qlik-bookmark-ui": ["src/libs/qlik-bookmark-ui/src"],
