{
    "name": "sh-poc-insight-frontend",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "tsc && vite build",
        "reset": "rm -rf node_modules && yarn",
        "start": "vite",
        "start-server": "tsc && vite build && node index.js",
        "start-server-no-build": "node index.js",
        "win-svc-install": "tsc && vite build && node ./service-install.js",
        "win-svc-uninstall": "node ./service-uninstall.js",
        "prepare": "husky && husky install",
        "lint": "eslint src/app/**/*.{ts,tsx}",
        "typecheck": "tsc --noEmit",
        "install-public": "yarn install --registry=https://registry.yarnpkg.com"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "dependencies": {
        "@draft-js-plugins/editor": "^4.1.3",
        "@draft-js-plugins/emoji": "^4.6.5",
        "@draft-js-plugins/mention": "^5.2.1",
        "@emotion/react": "^11.11.4",
        "@emotion/styled": "^11.11.5",
        "@mui/icons-material": "^5.16.4",
        "@mui/lab": "^5.0.0-alpha.163",
        "@mui/material": "^5.16.4",
        "@mui/system": "^5.16.4",
        "@mui/x-date-pickers": "^7.10.0",
        "autosuggest-highlight": "^3.1.1",
        "axios": "^1.7.7",
        "date-fns": "^3.6.0",
        "dayjs": "^1.11.11",
        "draft-js": "^0.11.7",
        "enigma.js": "^2.14.0",
        "i18next": "^22.5.0",
        "js-base64": "^3.7.2",
        "lodash-es": "^4.17.21",
        "moment": "^2.29.4",
        "query-string": "^9.0.0",
        "react": "^18.2.0",
        "react-content-loader": "^7.0.0",
        "react-dom": "^18.2.0",
        "react-draggable": "^4.4.3",
        "react-grid-layout": "^1.2.4",
        "react-html-parser": "^2.0.2",
        "react-i18next": "^14.1.1",
        "react-iframe": "^1.8.0",
        "react-json-view": "^1.21.3",
        "react-player": "^2.9.0",
        "react-responsive": "^10.0.0",
        "react-router-dom": "^6.23.1",
        "react-use": "^17.5.0",
        "react-window": "^1.8.6",
        "socket.io-client": "^4.7.5",
        "tslib": "^2.3.1",
        "tss-react": "^4.9.10",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^18.6.0",
        "@commitlint/config-conventional": "^18.6.0",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "12.1.2",
        "@testing-library/user-event": "13.5.0",
        "@types/autosuggest-highlight": "^3.2.3",
        "@types/draft-js": "^0.11.18",
        "@types/jest": "27.4.0",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "17.0.13",
        "@types/qlik-engineapi": "^12.67.16",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "@types/react-grid-layout": "^1.3.5",
        "@types/react-router-dom": "5.3.3",
        "@types/react-window": "^1.8.8",
        "@types/uuid": "^9.0.8",
        "@typescript-eslint/eslint-plugin": "^6.20.0",
        "@typescript-eslint/parser": "^6.20.0",
        "@vitejs/plugin-react-swc": "^4.2.2",
        "commitlint": "^20.2.0",
        "eslint": "^8.2.0",
        "eslint-config-airbnb": "19.0.4",
        "eslint-config-airbnb-typescript": "^18.0.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "husky": "^8.0.0",
        "lint-staged": "^15.2.1",
        "prettier": "^3.2.5",
        "react-inject-env": "^2.1.0",
        "typescript": "^5.3.3",
        "vite": "^7.3.0",
        "vite-plugin-mkcert": "^1.17.3",
        "vite-plugin-svgr": "^4.5.0",
        "vite-tsconfig-paths": "^6.0.3"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "prettier --write",
            "eslint --fix"
        ]
    },
    "resolutions": {
        "react-error-overlay": "6.0.9",
        "@types/react": "^18.2.0"
    }
}
